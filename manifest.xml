<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>7086d3d8-a0d5-4530-8eff-2abe78a90d7d</Id>
  <Version>2.0.0.0</Version>
  <ProviderName>Sefaria - Dicta Tools</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="כלי ספרייה ודיקטה"/>
  <Description DefaultValue="כלי לזיהוי והוספת מקורות מספרייה ודיקטה במסמכי Word"/>
  <IconUrl DefaultValue="https://daviddov.github.io/word_extantion/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://daviddov.github.io/word_extantion/assets/icon-64.png"/>
  <SupportUrl DefaultValue="https://github.com/Daviddov/word_extantion"/>
  <AppDomains>
    <AppDomain>https://daviddov.github.io</AppDomain>
    <AppDomain>https://www.sefaria.org</AppDomain>
    <AppDomain>https://talmudfinder-2-0.loadbalancer.dicta.org.il</AppDomain>
    <AppDomain>https://dicta.org.il</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Document"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://daviddov.github.io/word_extantion/index.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Document">
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <FunctionFile resid="Commands.Url"/>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabHome">
              <Group id="CommandsGroup">
                <Label resid="CommandsGroup.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <Control xsi:type="Button" id="TaskpaneButton">
                  <Label resid="TaskpaneButton.Label"/>
                  <Supertip>
                    <Title resid="TaskpaneButton.Label"/>
                    <Description resid="TaskpaneButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>ButtonId1</TaskpaneId>
                    <SourceLocation resid="Taskpane.Url"/>
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://daviddov.github.io/word_extantion/assets/icon-16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://daviddov.github.io/word_extantion/assets/icon-32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://daviddov.github.io/word_extantion/assets/icon-80.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://github.com/Daviddov/word_extantion"/>
        <bt:Url id="Commands.Url" DefaultValue="https://daviddov.github.io/word_extantion/src/commands/commands.html"/>
        <bt:Url id="Taskpane.Url" DefaultValue="https://daviddov.github.io/word_extantion/index.html"/>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="GetStarted.Title" DefaultValue="התחל להשתמש בכלי ספרייה ודיקטה!"/>
        <bt:String id="CommandsGroup.Label" DefaultValue="ספרייה ודיקטה"/>
        <bt:String id="TaskpaneButton.Label" DefaultValue="פתח כלי ספרייה ודיקטה"/>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="GetStarted.Description" DefaultValue="כלי ספרייה ודיקטה נטען בהצלחה. לך לתפריט הבית ולחץ על 'פתח כלי ספרייה ודיקטה' כדי להתחיל לזהות איזכורים ולהוסיף מקורות."/>
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="לחץ כדי לפתוח את כלי ספרייה ודיקטה - זיהוי איזכורים והוספת מקורות יהודיים"/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>